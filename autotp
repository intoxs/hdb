local placeid = 8888443753

local function autokick(plr)
    if iswindowactive() == true then
        game:GetService("StarterGui"):SetCore("PromptBlockPlayer", game.Players:GetPlayers()[math.random(2,#game.Players:GetPlayers())])
        spawn(function()
            while task.wait() do
            mousemoveabs(888, 665)
            mousemoveabs(888, 666)
            end
        end)
        spawn(function()
            while task.wait() do
                mouse1click()
            end
        end)
        task.wait(1)
        game:GetService("Players").LocalPlayer:Kick("Unwhitelisted Player Joined")
        task.wait(5)
        game:GetService("TeleportService"):Teleport(placeid, game:GetService("Players").LocalPlayer)
    end
end

print("injected")

while task.wait() do
    if game.PlaceId == placeid then
        game:GetService("Players").PlayerAdded:Connect(autokick)
    end
end
